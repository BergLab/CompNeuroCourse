   "source": [
    "# sampling rate\n",
    "sr = 100\n",
    "# sampling interval\n",
    "ts = 1.0/sr\n",
    "t = np.arange(0,2,ts)\n",
    "\n",
    "freq = 5.\n",
    "x = 3*np.sin(2*np.pi*freq*t + 3)\n",
    "\n",
    "freq = 2\n",
    "x += 2*np.sin(2*np.pi*freq*t - 2)\n",
    "\n",
    "\n",
    "plt.figure(figsize = (8, 6))\n",
    "plt.plot(t, x, 'r')\n",
    "plt.ylabel('Amplitude')\n",
    "plt.xlabel('Time (s)')\n",
    "plt.show()"
   ]
  },
