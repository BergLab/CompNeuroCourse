    "fig = plt.figure(figsize = (8,8))\n",
    "\n",
    "times = np.arange(5)\n",
    "\n",
    "n = len(times)\n",
    "\n",
    "for t in times:\n",
    "    plt.subplot(n, 1, t+1)\n",
    "    y = np.sin(x + t)\n",
    "    plt.plot(x, y, 'b')\n",
    "    plt.plot(x[25], y [25], 'ro')\n",
    "    plt.ylim(-1.1, 1.1)\n",
    "    plt.ylabel('y')\n",
    "    plt.title(f't = {t}')\n",
    "\n",
    "plt.xlabel('location (x)')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
