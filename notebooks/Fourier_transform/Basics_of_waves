   "source": [
    "# sampling rate\n",
    "sr = 100.0\n",
    "# sampling interval\n",
    "ts = 1.0/sr\n",
    "t = np.arange(0,1,ts)\n",
    "\n",
    "# frequency of the signal\n",
    "freq = 5   \n",
    "y = np.sin(2*np.pi*freq*t)\n",
    "\n",
    "plt.figure(figsize = (8, 8))\n",
    "plt.subplot(211)\n",
    "plt.plot(t, y, 'b')\n",
    "plt.ylabel('Amplitude')\n",
    "\n",
    "freq = 10   \n",
    "y = np.sin(2*np.pi*freq*t)\n",
    "\n",
    "plt.subplot(212)\n",
    "plt.plot(t, y, 'b')\n",
    "plt.ylabel('Amplitude')\n",
    "\n",
    "plt.xlabel('Time (s)')\n",
    "plt.show()"
   ]
  },
